{
  "name": "capacitor-plugin-shell-exec",
  "version": "0.0.1",
  "description": "Capacitor Plugin to Execute Commands in Android Operating System Shells",
  "main": "dist/plugin.js",
  "type": "module",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "EternalC0der",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^6.1.1",
    "@capacitor/core": "^6.1.1",
    "@capacitor/ios": "^6.1.1",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^22.0.2",
    "prettier": "^3.3.3",
    "prettier-plugin-java": "^2.6.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.2",
    "swiftlint": "^1.0.2",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.7"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorPluginShellExec.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/EternalC0der/capacitor-plugin-shell-exec"
  },
  "bugs": {
    "url": "https://github.com/EternalC0der/capacitor-plugin-shell-exec/issues"
  }
}
